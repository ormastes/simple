# 31_auto_forward_properties.spl - get_/set_/is_ forwarding

class Config:
    _port: i32

    fn get_port() -> i32:
        return self._port

    fn set_port(p: i32):
        self._port = p

let mut cfg = Config(_port: 8080)

# Property-style access should forward to get_/set_
print "port={cfg.port}"
cfg.port = 9090
print "port={cfg.port}"

main = cfg.port
