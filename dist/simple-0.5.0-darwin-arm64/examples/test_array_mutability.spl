class Counter:
    count: i64
    
    me increment():
        self.count = self.count + 1

fn main():
    var c = Counter(count: 0)
    print "Initial: {c.count}"
    
    var arr = [c]
    arr[0].increment()
    
    print "After modifying arr[0]: {c.count}"
    print "Value in array: {arr[0].count}"

main()
