{
  "threshold": 2.5,
  "reporters": ["html", "console"],
  "output": "target/duplication",
  "format": ["rust"],
  "ignore": [
    "**/target/**",
    "**/*.md",
    "**/gpu/src/backend/**"
  ],
  "minLines": 5,
  "minTokens": 55,
  "maxLines": 1000,
  "maxSize": "100kb",
  "absolute": false,
  "noSymlinks": true,
  "skipLocal": false,
  "formatsExts": {
    "rust": ["rs"]
  },
  "blame": false
}
