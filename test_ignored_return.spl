# Standalone test for ignored return value warnings

fn get_value() -> i64:
    42

fn get_name() -> text:
    "Alice"

fn do_work():
    print "working"

fn main():
    # This should warn
    get_value()

    # This should warn
    get_name()

    # This should NOT warn (void return)
    do_work()

    # This should NOT warn (value used)
    val x = get_value()

    print "Test complete"
