# Debug test file for testing JSON without spec framework
# This tests basic JSON functionality using simple checks

import core.json

fn main():
    # Test parsing null
    result = json.parse("null")
    if not result.is_ok():
        print("ERROR: Failed to parse null")
        return 1

    # Test parsing true
    result = json.parse("true")
    if not result.is_ok():
        print("ERROR: Failed to parse true")
        return 1

    # Test parsing integer
    result = json.parse("42")
    if not result.is_ok():
        print("ERROR: Failed to parse integer")
        return 1

    print("All JSON tests passed!")
    return 0
