# CBOR Utilities
import "std/cbor/core" as core

fn cbor_sequence_count(bytes: [i64]) -> i64: core.cbor_sequence_count(bytes)
fn cbor_sequence_item(bytes: [i64], index: i64) -> ([i64], i64): core.cbor_sequence_item(bytes, index)
fn cbor_to_diagnostic(bytes: [i64], offset: i64) -> (text, i64): core.cbor_to_diagnostic(bytes, offset)
fn cbor_hexdump(bytes: [i64]) -> text: core.cbor_hexdump(bytes)
