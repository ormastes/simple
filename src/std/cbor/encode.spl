# CBOR Encoding Functions
import "std/cbor/core" as core

fn cbor_encode_unsigned(value: i64) -> [i64]: core.cbor_encode_unsigned(value)
fn cbor_encode_int(value: i64) -> [i64]: core.cbor_encode_int(value)
fn cbor_encode_bytes(bytes: [i64]) -> [i64]: core.cbor_encode_bytes(bytes)
fn cbor_encode_text(text: text) -> [i64]: core.cbor_encode_text(text)
fn cbor_encode_array_header(length: i64) -> [i64]: core.cbor_encode_array_header(length)
fn cbor_encode_array_start() -> [i64]: core.cbor_encode_array_start()
fn cbor_encode_map_header(length: i64) -> [i64]: core.cbor_encode_map_header(length)
fn cbor_encode_map_start() -> [i64]: core.cbor_encode_map_start()
fn cbor_encode_break() -> [i64]: core.cbor_encode_break()
fn cbor_encode_false() -> [i64]: core.cbor_encode_false()
fn cbor_encode_true() -> [i64]: core.cbor_encode_true()
fn cbor_encode_bool(value: bool) -> [i64]: core.cbor_encode_bool(value)
fn cbor_encode_null() -> [i64]: core.cbor_encode_null()
fn cbor_encode_undefined() -> [i64]: core.cbor_encode_undefined()
fn cbor_encode_simple(value: i64) -> [i64]: core.cbor_encode_simple(value)
fn cbor_encode_float64(value: f64) -> [i64]: core.cbor_encode_float64(value)
fn cbor_encode_float32(value: f64) -> [i64]: core.cbor_encode_float32(value)
fn cbor_encode_tagged(tag: i64, value: [i64]) -> [i64]: core.cbor_encode_tagged(tag, value)
fn cbor_encode_timestamp(unix_seconds: i64) -> [i64]: core.cbor_encode_timestamp(unix_seconds)
fn cbor_encode_datetime_string(iso8601: text) -> [i64]: core.cbor_encode_datetime_string(iso8601)
fn cbor_encode_uri(uri: text) -> [i64]: core.cbor_encode_uri(uri)
fn cbor_encode_regex(pattern: text) -> [i64]: core.cbor_encode_regex(pattern)
fn cbor_encode_base64(data: text) -> [i64]: core.cbor_encode_base64(data)
fn cbor_encode_base64url(data: text) -> [i64]: core.cbor_encode_base64url(data)
fn cbor_encode_base16(data: text) -> [i64]: core.cbor_encode_base16(data)
fn cbor_encode_mime(mime_message: text) -> [i64]: core.cbor_encode_mime(mime_message)
fn cbor_encode_self_describe(value: [i64]) -> [i64]: core.cbor_encode_self_describe(value)
