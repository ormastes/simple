# K-d Tree Data Structure Module - Facade
# Comprehensive multidimensional search and spatial indexing utilities
# Binary space partitioning trees for efficient k-dimensional point operations

# Re-export all functionality from organized submodules
import kd_tree.types
import kd_tree.build
import kd_tree.search
import kd_tree.nearest_neighbor
import kd_tree.range
import kd_tree.insert
import kd_tree.delete
import kd_tree.utilities

# ============================================================================
# Types Module (kd_tree.types)
# ============================================================================
# Point operations: point_create, point_dimension, point_get, point_set
# point_equal, point_copy, point_distance_*
# KdNode operations: kdnode_create, kdnode_get_*, kdnode_set_*, kdnode_is_leaf
# BoundingBox operations: bbox_create, bbox_get_*, bbox_contains_point, bbox_intersects

# ============================================================================
# Build Module (kd_tree.build)
# ============================================================================
# Tree construction: kdtree_build, kdtree_build_recursive
# Structure operations: kdtree_height, kdtree_size, kdtree_to_list
# Rebalancing: kdtree_rebalance, kdtree_needs_rebalance
# Statistics: kdtree_depth, kdtree_leaf_count, kdtree_balance_factor
# Analysis: kdtree_dimension_bounds, kdtree_bounding_box, kdtree_dimension_variance
# Comparison: kdtree_points_equal, kdtree_subset, kdtree_merge

# ============================================================================
# Search Module (kd_tree.search)
# ============================================================================
# Exact search: kdtree_contains, kdtree_contains_recursive

# ============================================================================
# Nearest Neighbor Module (kd_tree.nearest_neighbor)
# ============================================================================
# Single nearest: kdtree_nearest_neighbor, kdtree_nearest_neighbor_*
# K-nearest: kdtree_k_nearest_neighbors

# ============================================================================
# Range Module (kd_tree.range)
# ============================================================================
# Range queries: kdtree_range_search, kdtree_range_search_radius
# Spatial queries: kdtree_count_in_range, kdtree_count_in_radius, kdtree_exists_in_range

# ============================================================================
# Insert Module (kd_tree.insert)
# ============================================================================
# Insertion: kdtree_insert, kdtree_bulk_insert

# ============================================================================
# Delete Module (kd_tree.delete)
# ============================================================================
# Deletion: kdtree_remove, kdtree_bulk_remove

# ============================================================================
# Utilities Module (kd_tree.utilities)
# ============================================================================
# Debugging: kdtree_print_node, kdtree_validate
# Point set operations: points_deduplicate, points_closest_pair
# points_centroid, points_farthest_from
