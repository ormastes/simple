# Trie Types and Node Creation
#
# Core trie data structure definitions and node creation functions.

# Creates a new empty trie
# Returns: Empty trie node (dict with 'children' and 'is_end' fields)
fn trie_create():
    {
        "children": {},
        "is_end": false
    }

# Creates a trie node
# Returns: Trie node with empty children and not marked as end
fn trie_node():
    {
        "children": {},
        "is_end": false
    }
