# CBOR Decoding Functions
import "std/cbor/core" as core

fn cbor_decode_unsigned(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_unsigned(bytes, offset)
fn cbor_decode_int(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_int(bytes, offset)
fn cbor_decode_bytes(bytes: [i64], offset: i64) -> ([i64], i64): core.cbor_decode_bytes(bytes, offset)
fn cbor_decode_text(bytes: [i64], offset: i64) -> (text, i64): core.cbor_decode_text(bytes, offset)
fn cbor_decode_array_header(bytes: [i64], offset: i64) -> (i64, i64, i64): core.cbor_decode_array_header(bytes, offset)
fn cbor_decode_map_header(bytes: [i64], offset: i64) -> (i64, i64, i64): core.cbor_decode_map_header(bytes, offset)
fn cbor_decode_bool(bytes: [i64], offset: i64) -> (bool, i64): core.cbor_decode_bool(bytes, offset)
fn cbor_decode_null(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_null(bytes, offset)
fn cbor_decode_undefined(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_undefined(bytes, offset)
fn cbor_decode_simple_value(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_simple_value(bytes, offset)
fn cbor_decode_tag(bytes: [i64], offset: i64) -> (i64, i64): core.cbor_decode_tag(bytes, offset)
