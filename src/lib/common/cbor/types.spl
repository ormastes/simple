# CBOR Types and Constants
import "std/cbor/core" as core

fn major_unsigned_int() -> i64: core.major_unsigned_int()
fn major_negative_int() -> i64: core.major_negative_int()
fn major_byte_string() -> i64: core.major_byte_string()
fn major_text_string() -> i64: core.major_text_string()
fn major_array() -> i64: core.major_array()
fn major_map() -> i64: core.major_map()
fn major_tag() -> i64: core.major_tag()
fn major_simple_float() -> i64: core.major_simple_float()

fn addl_immediate() -> i64: core.addl_immediate()
fn addl_uint8() -> i64: core.addl_uint8()
fn addl_uint16() -> i64: core.addl_uint16()
fn addl_uint32() -> i64: core.addl_uint32()
fn addl_uint64() -> i64: core.addl_uint64()
fn addl_indefinite() -> i64: core.addl_indefinite()

fn simple_false() -> i64: core.simple_false()
fn simple_true() -> i64: core.simple_true()
fn simple_null() -> i64: core.simple_null()
fn simple_undefined() -> i64: core.simple_undefined()
fn simple_float16() -> i64: core.simple_float16()
fn simple_float32() -> i64: core.simple_float32()
fn simple_float64() -> i64: core.simple_float64()
fn simple_break() -> i64: core.simple_break()
