# Sanitizer Shared Types
# @tag:api
#
# Common types used across all sanitizer modules (asan, lsan, ubsan, tsan, msan).

struct SanEvent:
    kind: text
    severity: text
    message: text
    location: text

fn san_event(kind: text, severity: text, message: text, location: text) -> SanEvent:
    SanEvent(kind: kind, severity: severity, message: message, location: location)

export SanEvent, san_event
