# FFI - CLI Commands
# Centralized CLI command delegation FFI wrappers
# These functions delegate to Rust-backed CLI command implementations

# ============================================================================
# Core CLI Functions
# ============================================================================

extern fn rt_cli_get_args() -> [str]

fn cli_get_args() -> [str]:
    rt_cli_get_args()

extern fn rt_cli_exit(code: i64)

fn cli_exit(code: i64):
    rt_cli_exit(code)

extern fn rt_cli_dispatch_rust(cmd: str, args: [str], gc_log: bool, gc_off: bool) -> i64

fn cli_dispatch_rust(cmd: str, args: [str], gc_log: bool, gc_off: bool) -> i64:
    rt_cli_dispatch_rust(cmd, args, gc_log, gc_off)

