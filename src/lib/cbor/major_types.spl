# CBOR Major Type Detection and Validation
import "std/cbor/core" as core

fn cbor_decode_type(bytes: [i64], offset: i64) -> (i64, i64, i64): core.cbor_decode_type(bytes, offset)
fn cbor_is_unsigned_int(bytes: [i64], offset: i64) -> bool: core.cbor_is_unsigned_int(bytes, offset)
fn cbor_is_negative_int(bytes: [i64], offset: i64) -> bool: core.cbor_is_negative_int(bytes, offset)
fn cbor_is_byte_string(bytes: [i64], offset: i64) -> bool: core.cbor_is_byte_string(bytes, offset)
fn cbor_is_text_string(bytes: [i64], offset: i64) -> bool: core.cbor_is_text_string(bytes, offset)
fn cbor_is_array(bytes: [i64], offset: i64) -> bool: core.cbor_is_array(bytes, offset)
fn cbor_is_map(bytes: [i64], offset: i64) -> bool: core.cbor_is_map(bytes, offset)
fn cbor_is_tagged(bytes: [i64], offset: i64) -> bool: core.cbor_is_tagged(bytes, offset)
fn cbor_is_simple(bytes: [i64], offset: i64) -> bool: core.cbor_is_simple(bytes, offset)
fn cbor_is_indefinite(bytes: [i64], offset: i64) -> bool: core.cbor_is_indefinite(bytes, offset)
fn cbor_is_break(bytes: [i64], offset: i64) -> bool: core.cbor_is_break(bytes, offset)
fn cbor_item_size(bytes: [i64], offset: i64) -> i64: core.cbor_item_size(bytes, offset)
fn cbor_validate(bytes: [i64]) -> bool: core.cbor_validate(bytes)
fn cbor_validate_sequence(bytes: [i64]) -> bool: core.cbor_validate_sequence(bytes)
fn cbor_is_canonical_int(value: i64, encoding: [i64]) -> bool: core.cbor_is_canonical_int(value, encoding)
