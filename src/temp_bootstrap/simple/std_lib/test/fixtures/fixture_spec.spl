describe "Fixture Tests":
    describe "using test data":
        it "tests with fixture value":
            let fixture_value = 42
            expect fixture_value == 42

        it "tests with fixture string":
            let fixture_name = "test_user"
            expect fixture_name == "test_user"

        it "tests with fixture list":
            let fixture_list = [1, 2, 3]
            expect fixture_list[0] == 1
            expect fixture_list[1] == 2
            expect fixture_list[2] == 3

    describe "computed fixtures":
        it "uses computed test data":
            let base = 10
            let multiplier = 5
            let expected = base * multiplier
            expect expected == 50
