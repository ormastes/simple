# Transform: parsing_to_desugaring
#
# Bridges parser output â†’ desugaring input.

arch {
  dimension = "transform"
  layer = "none"

  transform {
    from = "feature/parsing"
    to   = "feature/desugaring"
    allow_from = ["core/entity/ast/**"]
  }

  imports {
    allow = ["core/entity/ast/**", "shared/**"]
    deny  = ["compiler/feature/**"]
  }

  exports {
    expose = ["./entity_view/AstView"]
  }
}
