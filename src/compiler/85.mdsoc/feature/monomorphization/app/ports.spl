# Monomorphization feature ports
# Input: Typed AST with generic functions/types
# Output: Monomorphic AST with all generics specialized

struct MonomorphizeInputPort:
    """Typed AST ready for generic specialization."""
    typed_decl_count: i64
    generic_fn_count: i64
    has_generics: bool

struct MonomorphizeOutputPort:
    """Monomorphic AST with all generics resolved."""
    mono_decl_count: i64
    specialized_count: i64

export MonomorphizeInputPort, MonomorphizeOutputPort
