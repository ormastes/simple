# Adapters - Outbound Adapter Implementations
#
# Concrete implementations of port interfaces.
# Adapters connect the compiler to external systems (filesystem, network, etc.)
#
# out/ - outbound adapters (filesystem, memory, etc.)

arch {
  tier = "full"
  dimension = "adapter"
  layer = "infra"

  imports {
    allow = [
      "compiler/feature/**",
      "shared/**"
    ]
    deny = []
  }
}
