# Alloc NoGC - Arena/bump allocation without garbage collection
#
# Re-exports from core_nogc/ for tiered access.
# Import via: use std.alloc.nogc.*

#[no_gc]
#[variant(any)]
mod nogc

# Re-export no-GC collection types from core_nogc/
export core_nogc.arena.*
export core_nogc.bump.*
export core_nogc.fixed_vec.*
export core_nogc.small_vec.*
export core_nogc.small_map.*
export core_nogc.string_view.*
