# Bare-Metal Support Module
#
# Provides bare-metal programming support including:
# - Semihosting System API
# - Architecture-specific backends (ARM, RISC-V, x86)
# - String interning for optimized print output
#
# Usage:
#   from baremetal.system_api import {semi_print, semi_exit}
#   from baremetal.string_intern import {StringInternTable}

# Re-export main modules
export system_api
export string_intern

# Architecture-specific modules (conditionally compiled)
@cfg("target_arch", "arm")
export arm.semihost as arm_semihost

@cfg("target_arch", "riscv32")
@cfg("target_arch", "riscv64")
export riscv.semihost as riscv_semihost

@cfg("target_arch", "x86")
@cfg("target_arch", "i686")
@cfg("target_arch", "x86_64")
export x86.semihost as x86_semihost
