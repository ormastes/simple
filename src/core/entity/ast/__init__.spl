# AST Entity - Abstract Syntax Tree types
#
# Contents:
#   nodes.spl - CoreExpr/CoreStmt/CoreDecl structs
#   tags.spl  - EXPR_*/STMT_*/DECL_* constants
#   arena.spl - Parallel-array arenas

arch {
  dimension = "entity"
  layer = "entity"
  imports {
    allow = ["core/entity/token/**", "core/entity/**", "shared/**"]
    deny  = ["compiler/**", "feature/**"]
  }
  exports {
    expose = ["./arena", "./nodes", "./tags"]
  }
}
