use compiler.treesitter.*
use std.text.{NL}

val src = "fn main() -> i64:{NL}    0{NL}"
var ts = treesitter_new(src)
val outline = ts_parse_outline(ts)

print "imports: {outline.imports}"
print "exports: {outline.exports}"
print "functions: {outline.functions.len()}"
print "classes: {outline.classes}"
print "actors: {outline.actors}"
print "structs: {outline.structs}"
print "enums: {outline.enums}"
print "bitfields: {outline.bitfields}"
print "traits: {outline.traits}"
print "impls: {outline.impls}"
print "type_aliases: {outline.type_aliases}"
print "constants: {outline.constants}"
print "static_asserts: {outline.static_asserts}"
