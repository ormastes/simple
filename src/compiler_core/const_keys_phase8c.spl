# ============================================================================
# Result Type
# ============================================================================

enum Result:
    Ok(value: T)
    Err(error: E)

impl Result:
    fn is_ok() -> bool:
        match self:
            case Ok(_): true
            case _: false

    fn is_err() -> bool:
        match self:
            case Err(_): true
            case _: false

    fn unwrap() -> T:
        match self:
            case Ok(value): value
            case Err(_):
                assert false  # Called unwrap on Err
                # Unreachable but needed for type checking
                val dummy = 0
                dummy as T

    fn unwrap_err() -> E:
        match self:
            case Err(error): error
            case Ok(_):
                assert false  # Called unwrap_err on Ok
                val dummy = 0
                dummy as E

