# Simple CLI - todo-gen command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_todo_gen(args: [str]) -> i64

fn print_help():
    print "Usage: simple todo-gen [options]"
    print "Generates TODO report"
    print ""
    print "Run 'simple todo-gen --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_todo_gen(args)
