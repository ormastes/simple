# Simple CLI - diagram command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_diagram(args: [str]) -> i64

fn print_help():
    print "Usage: simple diagram [options]"
    print "Generates UML diagrams"
    print ""
    print "Run 'simple diagram --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_diagram(args)
