use compiler.frontend.frontend.*

extern fn sys_get_args() -> [text]

struct SimpleLogger:
    level: i64
    fn trace(msg: text):
        if self.level >= 20:
            print msg
    fn info(msg: text):
        if self.level >= 4:
            print msg

fn main() -> i64:
    val logger = SimpleLogger(level: 4)
    print "calling parse_full_frontend with wildcard import..."
    val ast = parse_full_frontend("fn main(): print 42", "test.spl", "main", logger)
    print "success!"
    return 0
