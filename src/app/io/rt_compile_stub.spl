# Pure Simple implementation of rt_compile_to_llvm_ir

use compiler.build.compile_to_llvm (compile_to_llvm_ir_pure)

fn rt_compile_to_llvm_ir(source_file: text, target_triple: text, bare_metal: bool) -> (text, text, i64):
    compile_to_llvm_ir_pure(source_file, target_triple, bare_metal)

export rt_compile_to_llvm_ir
