# Simple CLI - diff command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_diff(args: [str]) -> i64

fn print_help():
    print "Usage: simple diff [options]"
    print "Semantic diff between files"
    print ""
    print "Run 'simple diff --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_diff(args)
