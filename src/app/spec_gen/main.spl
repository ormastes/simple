# Simple CLI - spec-gen command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_spec_gen(args: [str]) -> i64

fn print_help():
    print "Usage: simple spec-gen [options]"
    print "Generates spec documentation"
    print ""
    print "Run 'simple spec-gen --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_spec_gen(args)
