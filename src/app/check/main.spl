# Simple CLI - check command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)

extern fn rt_cli_run_check(args: [str]) -> i64

fn print_help():
    print "Usage: simple check [options]"
    print "Type-checks source files without running"
    print ""
    print "Run 'simple check --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_check(args)
