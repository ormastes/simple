# Simple CLI - qualify-ignore command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_qualify_ignore(args: [str]) -> i64

fn print_help():
    print "Usage: simple qualify-ignore [options]"
    print "Manages qualified ignores"
    print ""
    print "Run 'simple qualify-ignore --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_qualify_ignore(args)
