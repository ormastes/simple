# Simple CLI - update command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_update(args: [str]) -> i64

fn print_help():
    print "Usage: simple update [options]"
    print "Updates dependencies"
    print ""
    print "Run 'simple update --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_update(args)
