# Simple CLI - migrate command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_migrate(args: [str]) -> i64

fn print_help():
    print "Usage: simple migrate [options]"
    print "Migration utilities"
    print ""
    print "Run 'simple migrate --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_migrate(args)
