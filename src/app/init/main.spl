# Simple CLI - init command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)

extern fn rt_cli_handle_init(args: [str]) -> i64

fn print_help():
    print "Usage: simple init [options]"
    print "Creates a new Simple project"
    print ""
    print "Run 'simple init --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_init(args)
