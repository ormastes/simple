# Simple CLI - feature-gen command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_feature_gen(args: [str]) -> i64

fn print_help():
    print "Usage: simple feature-gen [options]"
    print "Generates feature documentation"
    print ""
    print "Run 'simple feature-gen --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_feature_gen(args)
