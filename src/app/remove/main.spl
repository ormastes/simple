# Simple CLI - remove command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_remove(args: [str]) -> i64

fn print_help():
    print "Usage: simple remove [options]"
    print "Removes a dependency"
    print ""
    print "Run 'simple remove --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_remove(args)
