# Simple CLI - cache command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_cache(args: [str]) -> i64

fn print_help():
    print "Usage: simple cache [options]"
    print "Manages package cache"
    print ""
    print "Run 'simple cache --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_cache(args)
