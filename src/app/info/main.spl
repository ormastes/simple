# Simple CLI - info command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_info(args: [str]) -> i64

fn print_help():
    print "Usage: simple info [options]"
    print "Shows project and system information"
    print ""
    print "Run 'simple info --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_info(args)
