# Simple CLI - spec-coverage command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_spec_coverage(args: [str]) -> i64

fn print_help():
    print "Usage: simple spec-coverage [options]"
    print "Analyzes spec coverage"
    print ""
    print "Run 'simple spec-coverage --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_spec_coverage(args)
