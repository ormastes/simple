# Simple CLI - i18n command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_run_i18n(args: [str]) -> i64

fn print_help():
    print "Usage: simple i18n [options]"
    print "Internationalization tools"
    print ""
    print "Run 'simple i18n --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_run_i18n(args)
