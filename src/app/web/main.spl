# Simple CLI - web command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_web(args: [str]) -> i64

fn print_help():
    print "Usage: simple web [options]"
    print "Web framework operations"
    print ""
    print "Run 'simple web --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_web(args)
