# Simple CLI - lock command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_lock(args: [str]) -> i64

fn print_help():
    print "Usage: simple lock [options]"
    print "Lock file management"
    print ""
    print "Run 'simple lock --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_lock(args)
