# Simple CLI - env command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
extern fn rt_cli_handle_env(args: [str]) -> i64

fn print_help():
    print "Usage: simple env [options]"
    print "Manages virtual environments"
    print ""
    print "Run 'simple env --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    rt_cli_handle_env(args)
