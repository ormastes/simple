import intern_codegen

use app.io.mod (file_read)

fn main():
    val source: text = file_read("src/app/ffi_gen/specs/io_print.spl")
    if source == "":
        print "Failed to read file"
        return
    print "Source length: {source.len()}"
    val specs = intern_codegen.parse_intern_specs(source)
    print "Specs: {specs.len()}"

main()
