# Simple CLI - gen-lean command
# Delegates to Rust runtime via FFI

use app.cli_util (get_cli_args)
use app.io.mod (cli_gen_lean)

fn print_help():
    print "Usage: simple gen-lean [options]"
    print "Generates Lean 4 verification files"
    print ""
    print "Run 'simple gen-lean --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    cli_gen_lean(args)
