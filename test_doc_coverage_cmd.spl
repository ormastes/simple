# Test doc coverage command
use app.doc_coverage.scanner.mod.{DocItem, scan_file_for_docs}

fn main() -> i64:
    val items = scan_file_for_docs("src/std/string.spl")
    print "Found {items.len()} items"
    if items.len() > 0:
        val item = items[0]
        val has_doc = item.has_comment or item.has_docstring
        print "First item: {item.name}, documented: {has_doc}"
    0
