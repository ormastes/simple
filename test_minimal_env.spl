# Minimal env test - direct FFI
use spec.{describe, it, expect}

extern fn rt_env_get(key: text) -> text

fn get_env(key: text) -> text:
    val result = rt_env_get(key)
    if result == nil:
        ""
    else:
        result

describe "Test":
    it "works":
        val path = get_env("PATH")
        print "PATH length: {path.len()}"
        expect(path).to_not_equal("")
