#!/usr/bin/env simple
# Test if module-level generic functions work

use std.spec

# Define a simple generic function at module level
fn test_generic<T>(value: T) -> T:
    value

describe "Generic Module Function Test":
    it "calls module-level generic function":
        val result = test_generic(42)
        check(result == 42)

    it "calls with different type":
        val result = test_generic("hello")
        check(result == "hello")
