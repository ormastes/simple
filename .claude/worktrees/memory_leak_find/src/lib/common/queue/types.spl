# Queue Data Structure Types
#
# Type definitions and constructors for various queue structures

# Create a new empty queue (FIFO)
fn queue_new():
    (items: [], )

# Create a new empty stack (LIFO)
fn stack_new():
    (items: [], )

# Create a new empty deque (double-ended queue)
fn deque_new():
    (items: [], )

# Create a new empty priority queue
fn pqueue_new():
    (items: [], )

# Create a new circular buffer with given capacity
fn cbuffer_new(capacity):
    (items: [], capacity: capacity, head: 0, tail: 0, full: false, )

export queue_new
export stack_new
export deque_new
export pqueue_new
export cbuffer_new
