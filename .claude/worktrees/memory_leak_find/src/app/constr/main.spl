# Simple CLI - constr command
# Delegates to Rust runtime via FFI

use std.cli.cli_util (get_cli_args)
use app.io.mod (cli_constr)

fn print_help():
    print "Usage: simple constr [options]"
    print "Constraint analysis"
    print ""
    print "Run 'simple constr --help' for Rust-level help."

fn main() -> i64:
    val args = get_cli_args()
    cli_constr(args)
