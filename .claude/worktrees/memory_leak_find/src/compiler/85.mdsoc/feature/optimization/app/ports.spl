# Optimization feature ports
# Input: MIR program ready for optimization
# Output: Optimized MIR program

struct OptimizeInputPort:
    """MIR program input for optimization passes."""
    mir_fn_count: i64
    mir_inst_count: i64
    optimization_level: i64

struct OptimizeOutputPort:
    """Optimized MIR program."""
    mir_fn_count: i64
    mir_inst_count: i64
    eliminated_count: i64

export OptimizeInputPort, OptimizeOutputPort
