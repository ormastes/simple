# Transform: desugaring_to_typing
#
# Bridges desugaring output â†’ type checking input.

arch {
  dimension = "transform"
  layer = "none"

  transform {
    from = "feature/desugaring"
    to   = "feature/type_checking"
    allow_from = ["core/entity/ast/**"]
  }

  imports {
    allow = ["core/entity/ast/**", "shared/**"]
    deny  = ["compiler/feature/**"]
  }

  exports {
    expose = ["./entity_view/DesugarView"]
  }
}
