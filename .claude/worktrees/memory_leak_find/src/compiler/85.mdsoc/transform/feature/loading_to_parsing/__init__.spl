# Transform: loading_to_parsing
#
# Bridges module loader output â†’ parser input.
# Carries the loaded module source text and module name
# from the module loader to the parser pipeline stage.
# Design: doc/research/compiler_mdsoc_design.md Section 2.2

arch {
  dimension = "transform"
  layer = "none"

  transform {
    from = "feature/module_loading"
    to   = "feature/parsing"
    allow_from = ["core/entity/token/**", "core/entity/ast/**"]
  }

  imports {
    allow = ["core/entity/**", "shared/**"]
    deny  = ["compiler/feature/**"]
  }

  exports {
    expose = ["./entity_view/LoadedModuleView"]
  }
}
