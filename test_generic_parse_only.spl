# Test: Parse generic class syntax

use core.parser.{parse_module, parser_has_errors, parser_get_errors}

describe "Runtime Generic Parsing":
    it "can parse class with generic parameter":
        val source = "class Box<T>:\n    value: T\n"
        parse_module(source, "test.spl")
        val has_err = parser_has_errors()
        if has_err:
            val errors = parser_get_errors()
            print "Parse errors: {errors}"
        expect(has_err).to_equal(false)
