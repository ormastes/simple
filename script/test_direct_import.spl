#!/usr/bin/env simple
# Test direct import from lazy_seq module

use std.spec
use app.interpreter.lazy.lazy_seq.{LazySeq__empty, LazySeq__single}

describe "Direct Import Test":
    it "calls LazySeq__empty directly":
        val seq = LazySeq__empty()
        print "Created empty sequence"
        check(true)

    it "calls LazySeq__single directly":
        val seq = LazySeq__single(42)
        print "Created single-element sequence"
        check(true)
