# Test Phase 5: Self Return Type - Parser Verification

struct Point:
    x: i32
    y: i32

impl Point:
    # Self return type in method signature
    fn move_right() -> self:
        return Point(x: self.x + 1, y: self.y)

    fn move_up() -> self:
        return Point(x: self.x, y: self.y + 1)

    fn scale(factor: i32) -> self:
        return Point(x: self.x * factor, y: self.y * factor)

print("✅ Parser successfully accepted 'fn method() -> self' syntax!")
print("✅ Phase 5 implementation complete: Type::SelfType added and recognized")
