print("testing scan result mutation")

class TestResult:
    files: List[String]
    
    fn new() -> TestResult:
        return TestResult(files: [])

let mut result = TestResult.new()
print("Initial files count: {result.files.len()}")

result.files.push("file1.spl")
print("After push 1: {result.files.len()}")

result.files.push("file2.spl")
print("After push 2: {result.files.len()}")

for f in result.files:
    print("  - {f}")

print("done")
