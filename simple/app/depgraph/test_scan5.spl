print("testing class field mutation via assignment")

class TestResult:
    files: List[String]
    
    fn new() -> TestResult:
        return TestResult(files: [])

let mut result = TestResult.new()
print("Initial files count: {result.files.len()}")

# Try assigning the result of push
let mut f = result.files
f.push("file1.spl")
result.files = f
print("After push and assign: {result.files.len()}")

print("done")
