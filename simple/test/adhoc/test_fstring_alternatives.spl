val name = "Lexer"
val id = 1

# Test 1: Regular triple-quoted string (no f-string)
val regular = """# {name}
**ID:** #{id}
"""
print("=== Regular triple-quote (no interpolation) ===")
print(regular)

# Test 2: Multi-line with escaped newlines in f-string
val escaped = f"# {name}\n\n**ID:** #{id}\n"
print("=== F-string with \\n ===")
print(escaped)

# Test 3: Build with concatenation
val built = f"# {name}" + "\n\n" + f"**ID:** #{id}\n"
print("=== Built with concatenation ===")
print(built)
