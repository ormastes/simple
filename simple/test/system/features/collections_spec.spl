# Feature Test: Collections
# Returns 0 for success, 1 for failure

# Test arrays
let arr = [1, 2, 3, 4, 5]
if arr.len() != 5:
    main = 1
else:
    if arr[0] != 1:
        main = 1
    else:
        # Test array push
        arr.push(6)
        if arr.len() != 6:
            main = 1
        else:
            # Test dict
            let dict = {name: "Alice", age: 30}
            if dict["name"] != "Alice":
                main = 1
            else:
                if dict["age"] != 30:
                    main = 1
                else:
                    # Test range iteration
                    let sum = 0
                    for i in range(1, 4):  # 1, 2, 3
                        sum = sum + i
                    if sum != 6:
                        main = 1
                    else:
                        main = 0  # All tests passed
