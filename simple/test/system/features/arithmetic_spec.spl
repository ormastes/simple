# Feature Test: Arithmetic Operations
# Returns 0 for success, 1 for failure

# Test addition
let result1 = 2 + 3
if result1 != 5:
    main = 1  # Failure
else:
    # Test subtraction
    let result2 = 10 - 3
    if result2 != 7:
        main = 1
    else:
        # Test multiplication
        let result3 = 4 * 5
        if result3 != 20:
            main = 1
        else:
            # Test division
            let result4 = 15 / 3
            if result4 != 5:
                main = 1
            else:
                # Test precedence
                let result5 = 2 + 3 * 4
                if result5 != 14:
                    main = 1
                else:
                    main = 0  # All tests passed
