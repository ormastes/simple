# Test macro contract symbol introduction (#1303)

macro gen_function(name: Str const, value: Int const) -> (
    intro my_func:
        enclosing.module.fn "{name}"() -> Int
):
    emit my_func:
        fn "{name}"() -> Int:
            return value

# Call the macro to introduce get_answer function
gen_function!("get_answer", 42)

# Now the function should be available
fn main():
    let result = get_answer()
    println!("Result: ", result)
    assert_eq!(result, 42)
