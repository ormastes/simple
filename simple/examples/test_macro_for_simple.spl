# Minimal test for const-eval for loop

macro gen_getters() -> (
    intro funcs: for i in 0..2: enclosing.module.fn "get_{i}"() -> Int
):
    emit funcs:
        fn get_0() -> Int:
            return 0
        fn get_1() -> Int:
            return 1

gen_getters!()

fn main():
    let v0 = get_0()
    let v1 = get_1()
    println!("v0=", v0, " v1=", v1)
    assert_eq!(v0, 0)
    assert_eq!(v1, 1)
    println!("Success!")
