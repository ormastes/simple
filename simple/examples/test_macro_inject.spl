# Test macro inject functionality (here anchor)

macro debug_here() -> (
    inject debug_stmt:
        callsite.block.here.stmt
):
    emit debug_stmt:
        println!("DEBUG: Macro injected code executing here")

fn test_function():
    println!("Before macro invocation")
    debug_here!()
    println!("After macro invocation")
    let x = 10
    let y = 20
    println!("x + y = ", x + y)

fn main():
    test_function()
