# Test more specific syntax patterns

# Test: Tuple return type
fn test_tuple_return() -> (bool, str):
    return (true, "success")

# Test: Method calls
fn test_method_calls() -> bool:
    val s = "hello"
    val len = s.len()
    return len > 0

# Test: Array with explicit type and size (Rust-style)
