describe "Primitives":
    describe "integers":
        it "handles positive integers":
            expect 42 == 42
        it "handles zero":
            expect 0 == 0
        it "handles negative with parens":
            expect (0 - 1) == (0 - 1)

    describe "booleans":
        it "handles true":
            expect true == true
        it "handles false":
            expect false == false
        it "negates correctly":
            expect (not false)
