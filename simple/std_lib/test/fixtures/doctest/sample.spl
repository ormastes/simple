/// Computes factorial of n
/// 
/// Examples:
/// >>> factorial 5
/// 120
/// >>> factorial 0
/// 1
/// >>> factorial 1
/// 1
fn factorial(n: Int) -> Int:
    if n <= 1:
        return 1
    return n * factorial(n - 1)

/// Stack data structure
/// 
/// Examples:
/// >>> s = Stack.new()
/// >>> s.push(42)
/// >>> s.pop()
/// 42
/// >>> s.is_empty()
/// True
struct Stack:
    items: List[Int]
    
    fn new() -> Stack:
        return Stack(items: [])
    
    fn push(self, item: Int):
        self.items.push(item)
    
    fn pop(self) -> Int:
        return self.items.pop()
    
    fn is_empty(self) -> Bool:
        return self.items.len == 0
