# Test string concatenation patterns

fn test_simple_concat() -> str:
    return "Hello" + " " + "World"

fn test_multiline_concat() -> str:
    return "Line 1\n" +
           "Line 2\n" +
           "Line 3"

fn main():
    val s1 = test_simple_concat()
    print(s1)

    val s2 = test_multiline_concat()
    print(s2)
