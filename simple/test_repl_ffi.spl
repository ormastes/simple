# Test REPL FFI - minimal test

fn test_repl_init() -> bool:
    # Try calling the FFI function directly
    # In Simple, extern functions might be called directly
    val result = simple_repl_runner_init()
    return result

fn main():
    val success = test_repl_init()
    if success:
        print("REPL runner initialized successfully")
        simple_repl_runner_cleanup()
    else:
        print("Failed to initialize REPL runner")
