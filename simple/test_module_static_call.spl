# Test calling static method on imported class
# This simulates: import physics.core as core; core.Vector3::zero()

class Vector3:
    x: float
    y: float
    z: float

    @staticmethod
    fn zero() -> Vector3:
        return Vector3(0.0, 0.0, 0.0)

fn main():
    # Test 1: Direct call (known to work)
    val v1 = Vector3::zero()
    print("Direct call works: " + str(v1.x))

    # Test 2: Assign class to variable, then call static method
    val vec_class = Vector3
    # This won't work with :: syntax on a variable
    # val v2 = vec_class::zero()  # Expected to fail

    print("Test completed")
    return 0
