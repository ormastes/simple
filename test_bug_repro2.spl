# Test file to reproduce parser bug parser_004 only

# Bug parser_004: ? in for loop
fn test_try_in_loop(items: [text]) -> Result<i64, text>:
    var total = 0
    for item in items:
        val n = item.parse_int()?
        total = total + n
    Ok(total)

fn main():
    print "Testing bug parser_004..."

    val result = test_try_in_loop(["1", "2", "3"])
    print "Result: {result}"
