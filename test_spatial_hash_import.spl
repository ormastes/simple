# Test that spatial hash imports work correctly
import physics.collision as collision
import physics.core as core

# Test SpatialHash
let spatial_hash = collision.SpatialHash(cell_size=2.0)
spatial_hash.insert(0, core.Vector3(0, 0, 0), 1.0)
spatial_hash.insert(1, core.Vector3(2, 0, 0), 1.0)
let pairs = spatial_hash.get_potential_pairs()
print(f"Pairs: {pairs.len()}")

# Test ConvexHull
let points = [
    core.Vector3(0, 0, 0),
    core.Vector3(1, 0, 0),
    core.Vector3(0, 1, 0),
    core.Vector3(0, 0, 1)
]
let hull = collision.ConvexHull::from_points(points)
print(f"Hull vertices: {hull.vertices.len()}")

# Test SpatialHashGrid
let grid = collision.SpatialHashGrid(cell_size=1.0)
let aabb = collision.AABB::from_center_size(core.Vector3(0, 0, 0), core.Vector3(1, 1, 1))
grid.insert(0, aabb)
print(f"Grid objects: {grid.num_objects()}")

print("All imports successful!")
