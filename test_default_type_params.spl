# Test default type parameters

# Basic default type parameter
trait Add<Rhs = Self>:
    type Output
    fn add(rhs: Rhs) -> Self::Output

# Multiple type parameters with defaults
trait Convert<From, To = Self>:
    fn convert(value: From) -> To

# Mix of regular and default type parameters
trait Combine<T, U = T, V = U>:
    fn combine(a: T, b: U, c: V) -> T

# Default type parameter using another generic
trait Process<Input, Output = Input>:
    fn process(input: Input) -> Output
