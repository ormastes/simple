# Test regex with debug output
import std.core.regex

print("Step 1: Creating parser...")
val parser = regex.RegexParser.new("h")
print("Step 2: Parser created, pos = {parser.pos}")

print("Step 3: Parsing...")
val ast = parser.parse()
print("Step 4: Parsed!")

print("Step 5: Creating NFA builder...")
val builder = regex.NFABuilder.new()
print("Step 6: Builder created")

print("Step 7: Building NFA...")
val nfa = builder.build(ast)
print("Step 8: NFA built!")

print("Done!")
