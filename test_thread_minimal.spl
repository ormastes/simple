# Minimal thread test to verify parser fix

use std.thread_sffi.{thread_cpu_count, thread_sleep}

fn main():
    print "Testing thread_cpu_count..."
    val count = thread_cpu_count()
    print "CPU count: {count}"

    print "Testing thread_sleep..."
    thread_sleep(10)
    print "Sleep complete"

    print "Thread SFFI works!"

main()
