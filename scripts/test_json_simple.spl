# Simple JSON test

import std.json.*

# Test parsing
print "Testing JSON parser..."
val result = JsonValue.parse("{\"name\": \"Alice\", \"age\": 30}")
match result:
    case Result.Ok(json):
        print "✓ Parsed successfully"
        print "Serialized: {json.serialize()}"
    case Result.Err(e):
        print "✗ Parse failed: {e}"

# Test parse_int
val num = parse_int("42")
print "parse_int(\"42\"): {num}"
