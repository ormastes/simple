# Math Aliases - Backward compatibility type aliases
#
# Maps physics-style names (Vector2, Vector3, etc.) to math module types.
# All aliases point to f64 variants to match physics module conventions.

export Vector2, Vector3, Matrix3, Matrix4, Quaternion

use .vec2
use .vec3
use .mat3
use .mat4
use .quat

# Physics module backward compatibility aliases (f64 precision)
type Vector2 = vec2.Vec2d
type Vector3 = vec3.Vec3d
type Matrix3 = mat3.Mat3d
type Matrix4 = mat4.Mat4d
type Quaternion = quat.Quatd
