# Test case for parser bug: field access on generic class parameters

class Box:
    value: i64

fn get_value_concrete(box: Box) -> i64:
    box.value  # This should work

describe "Generic field access bug":
    it "works with concrete type":
        val x = Box(value: 42)
        val result = get_value_concrete(x)
        expect(result).to_equal(42)
