# Debug test for false positives
import std.core.regex

print("Compiling pattern 'xyz'...")
let pattern = regex.compile("xyz")
print("Pattern compiled")

print("Searching in 'hello world'...")
let result = pattern.search("hello world")
print("Search complete")

if result.is_some():
    let m = result.unwrap()
    print("UNEXPECTED: Found match")
    print("  text: {m.text}")
    print("  start: {m.start}")
    print("  end: {m.end}")
else:
    print("CORRECT: No match found")
