import config

let cfg_dict = {"test": 123, "nested": {"value": 456}}
let cfg = config.from_dict(cfg_dict)
print(f"Config get test: {cfg.get('test')}")
print(f"Config get nested: {cfg.get('nested.value')}")

let cfg2 = config.from_dict({"test": 999})
let merged = config.merge(cfg, cfg2)
print(f"Merged test: {merged.get('test')}")
