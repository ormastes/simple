# Test: Lexer Generic Context Tracking - Simple Version
#
# Tests token constants to verify TOK_LT_GENERIC is defined

use core.tokens.{TOK_LT, TOK_LT_GENERIC, TOK_GT}

fn test_token_constants():
    print "TOK_LT = {TOK_LT}"
    print "TOK_LT_GENERIC = {TOK_LT_GENERIC}"
    print "TOK_GT = {TOK_GT}"

    val different: bool = TOK_LT != TOK_LT_GENERIC
    if different:
        print "SUCCESS: TOK_LT and TOK_LT_GENERIC are different tokens"
    else:
        print "FAIL: TOK_LT and TOK_LT_GENERIC have same value"

test_token_constants()
