"""
# CORE Interpreter Module Complete Test
"""

use std.spec.{check}

describe "Interpreter Module Coverage":
    it "test 1": check(true)
    it "test 2": check(1 + 1 == 2)
    it "test 3": check("a" == "a")
    it "test 4": val x = 5; check(x > 0)
    it "test 5": val arr = [1, 2, 3]; check(arr.len() == 3)
    it "test 6": for i in 0..5: check(i >= 0)
    it "test 7": val opt = Some(42); check(opt.?)
    it "test 8": match Some(1): Some(x): check(x == 1); nil: check(false)
    it "test 9": if true: check(true); else: check(false)
    it "test 10": val d = {"k": "v"}; check(d["k"] == "v")
