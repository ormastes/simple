# Test Group 8: Range Operations
# ============================================================================

describe "Range Operations":
    """
    ## Range Operations

    Tests range creation and iteration.
    """

    it "iterates exclusive range":
        var sum = 0
        for i in 0..5:
            sum = sum + i
        expect(sum).to_equal(10)  # 0+1+2+3+4

    it "iterates inclusive range":
        var sum = 0
        for i in 0..=5:
            sum = sum + i
        expect(sum).to_equal(15)  # 0+1+2+3+4+5

    it "handles single-element range":
        var count = 0
        for _ in 5..6:
            count = count + 1
        expect(count).to_equal(1)

    it "handles empty range":
        var count = 0
        for _ in 5..5:
            count = count + 1
        expect(count).to_equal(0)


