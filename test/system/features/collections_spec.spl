describe "Dictionary Primitives":
    """
    ## Dictionary Primitives

    Basic dictionary creation, insertion, and retrieval.
    """

    context "dictionaries":
        it "stores and retrieves keys":
            val dict = {"name": "Alice", "age": 30}
            expect(dict["name"]).to_equal("Alice")
            expect(dict["age"]).to_equal(30)

        it "handles empty dictionary":
            val dict: {text: i64} = {}
            expect(dict.len()).to_equal(0)

        it "inserts new key":
            var dict: {text: i64} = {}
            dict["key"] = 42
            expect(dict["key"]).to_equal(42)

        it "updates existing key":
            var dict = {"a": 1}
            dict["a"] = 2
            expect(dict["a"]).to_equal(2)

        it "handles multiple keys":
            var dict: {text: text} = {}
            dict["a"] = "apple"
            dict["b"] = "banana"
            dict["c"] = "cherry"
            expect(dict.len()).to_equal(3)


