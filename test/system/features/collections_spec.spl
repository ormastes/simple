# Test Group 5: Array Iteration
# ============================================================================

describe "Array Iteration":
    """
    ## Array Iteration

    Tests looping and iteration over arrays.
    """

    context "ranges and loops":
        it "sums a small range":
            val sum = 0
            for i in [1, 2, 3]:
                sum = sum + i
            expect(sum).to_equal(6)

        it "iterates array with modification":
            var total = 0
            for item in [10, 20, 30]:
                total = total + item
            expect(total).to_equal(60)

        it "collects iteration results":
            val numbers = [1, 2, 3, 4, 5]
            var results = []
            for n in numbers:
                results.push(n * 2)
            expect(results.len()).to_equal(5)

        it "counts iterations":
            var count = 0
            for _ in [0, 1, 2, 3]:
                count = count + 1
            expect(count).to_equal(4)


# ============================================================================
# Test Group 6: Array Methods - Map and Filter
# ============================================================================

describe "Array Transformation Methods":
    """
    ## Array Transformation Methods

    Tests map, filter, and similar transformation operations.
    """

    it "maps with lambda":
        val nums = [1, 2, 3]
        val doubled = nums.map(\x: x * 2)
        expect(doubled).to_equal([2, 4, 6])

    it "filters with lambda":
        val nums = [1, 2, 3, 4, 5]
        val evens = nums.filter(\x: x % 2 == 0)
        expect(evens.len()).to_equal(2)

    it "chains map and filter":
        val nums = [1, 2, 3, 4, 5]
        val result = nums.filter(\x: x > 2).map(\x: x * 2)
        expect(result == [6, 8, 10]).to_equal(true)


