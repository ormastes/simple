"""
# Final System Test

**Feature IDs:** #FINAL
**Category:** Testing
**Status:** Implemented
"""

use std.spec.{check}

describe "Final System Test":
    slow_it "complete system check 1":
        val result = 1 + 1
        check(result == 2)
        
    slow_it "complete system check 2":
        val arr = [1, 2, 3, 4, 5]
        var sum = 0
        for x in arr:
            sum = sum + x
        check(sum == 15)
        
    slow_it "complete system check 3":
        val opt = Some(100)
        match opt:
            Some(x): check(x == 100)
            nil: check(false)
            
    slow_it "complete system check 4":
        var state = "start"
        if state == "start":
            state = "processing"
        if state == "processing":
            state = "done"
        check(state == "done")
        
    slow_it "complete system check 5":
        val dict = {"a": 1, "b": 2, "c": 3}
        val keys = dict_keys(dict)
        check(keys.len() == 3)
        
    slow_it "complete system check 6":
        var count = 0
        for i in 0..20:
            if i % 2 == 0:
                count = count + 1
        check(count == 10)
        
    slow_it "complete system check 7":
        val nested = [[1, 2], [3, 4], [5, 6]]
        check(nested.len() == 3)
        check(nested[0].len() == 2)
        
    slow_it "complete system check 8":
        val s = "test string"
        check(s.len() == 11)
        check(s.contains("test"))
        
    slow_it "complete system check 9":
        val a = 10
        val b = 20
        check(a < b)
        check(b > a)
        check(a + b == 30)
        
    slow_it "complete system check 10":
        var results = []
        for i in 0..5:
            results = results.append(i * i)
        check(results.len() == 5)
        check(results[4] == 16)
