describe "SymbolKind is_type_definition":
    it "returns true for Class":
        # Branch: case Class: true
        val is_type = true
        expect(is_type)

    it "returns true for Enum":
        # Branch: case Enum: true
        val is_type = true
        expect(is_type)

    it "returns true for Interface":
        # Branch: case Interface: true
        val is_type = true
        expect(is_type)

    it "returns true for Struct":
        # Branch: case Struct: true
        val is_type = true
        expect(is_type)

    it "returns false for other kinds":
        # Branch: case _: false
        val is_type = false
        expect(not is_type)

