# Debug: Test lexer imports

use compiler.lexer.*
use compiler.lexer_types.*
use compiler.blocks.modes.{LexerMode}

describe "Import test":
    it "can access TokenKind enum":
        val kind = TokenKind.KwFn
        expect kind == TokenKind.KwFn

    it "can access is_digit function":
        expect is_digit('5')
        expect not is_digit('a')

    it "can access Lexer struct":
        val lexer = Lexer.new("test")
        expect lexer.pos == 0
