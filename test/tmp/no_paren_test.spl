"""Test no-paren method calls"""

it "list.len works without parens":
    val list = [1, 2, 3]
    expect(list.len == 3)

it "string.len works without parens":
    val str = "hello"
    expect(str.len == 5)

it "comparison with no-paren method call":
    val items = [1, 2, 3, 4, 5]
    if items.len > 3:
        expect(true)
    else:
        expect(false)
