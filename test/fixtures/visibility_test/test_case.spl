# Test module with mixed visibility

# Auto-public: matches filename (test_case.spl -> TestCase)
class TestCase:
    id: i32
    name: text

    fn get_id() -> i32:
        self.id

# Private: doesn't match filename
class Helper:
    data: i32

    fn process() -> i32:
        self.data * 2

# Explicitly public
pub class Utils:
    x: i32

    fn double() -> i32:
        self.x * 2

# Private constant
val DEFAULT_ID: i32 = 0

# Public constant
pub val MAX_ID: i32 = 9999

# Private function
fn internal_helper() -> i32:
    42

# Public function
pub fn create_test(id: i32, name: text) -> TestCase:
    TestCase(id: id, name: name)
