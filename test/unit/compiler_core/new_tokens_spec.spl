describe "New Compile-Time Keyword Tokens":
    it "static_for has token 201":
        use compiler.core.tokens.{TOK_KW_STATIC_FOR}
        expect TOK_KW_STATIC_FOR == 201

    it "keyword_lookup finds static_for":
        use compiler.core.tokens.{TOK_KW_STATIC_FOR, keyword_lookup}
        expect keyword_lookup("static_for") == TOK_KW_STATIC_FOR

    it "tok_kind_name returns static_for":
        use compiler.core.tokens.{TOK_KW_STATIC_FOR, tok_kind_name}
        expect tok_kind_name(TOK_KW_STATIC_FOR) == "static_for"

    it "comptime has token 202":
        use compiler.core.tokens.{TOK_KW_COMPTIME}
        expect TOK_KW_COMPTIME == 202

    it "keyword_lookup finds comptime":
        use compiler.core.tokens.{TOK_KW_COMPTIME, keyword_lookup}
        expect keyword_lookup("comptime") == TOK_KW_COMPTIME

    it "tok_kind_name returns comptime":
        use compiler.core.tokens.{TOK_KW_COMPTIME, tok_kind_name}
        expect tok_kind_name(TOK_KW_COMPTIME) == "comptime"

    it "mixin has token 203":
        use compiler.core.tokens.{TOK_KW_MIXIN}
        expect TOK_KW_MIXIN == 203

    it "keyword_lookup finds mixin":
        use compiler.core.tokens.{TOK_KW_MIXIN, keyword_lookup}
        expect keyword_lookup("mixin") == TOK_KW_MIXIN

    it "tok_kind_name returns mixin":
        use compiler.core.tokens.{TOK_KW_MIXIN, tok_kind_name}
        expect tok_kind_name(TOK_KW_MIXIN) == "mixin"
