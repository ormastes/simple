# Test module for export...as bug reproduction

fn original_function(x: i64) -> i64:
    # Original function that we want to alias.
    x * 2

fn another_function(x: i64) -> i64:
    # Another function for testing.
    x + 10

# This SHOULD work but causes parse error in runtime:
# export original_function as aliased_function
# export another_function as renamed_function

# Current workaround - wrapper functions:
fn aliased_function(x: i64) -> i64:
    # Wrapper for original_function (workaround for export...as bug).
    original_function(x)

fn renamed_function(x: i64) -> i64:
    # Wrapper for another_function (workaround for export...as bug).
    another_function(x)

export aliased_function, renamed_function
