# ce NAME: block parser spec
use core.tokens.{TOK_KW_CE}
use core.ast.{DECL_CE}

describe "ce NAME: block parsing":
    it "DECL_CE has value 11":
        expect(DECL_CE).to_equal(11)

    it "TOK_KW_CE has value 205":
        expect(TOK_KW_CE).to_equal(205)

    it "DECL_CE is distinct from DECL_FN":
        val decl_fn = 1
        expect(DECL_CE != decl_fn).to_equal(true)
