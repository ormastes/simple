# bind statement parser spec
use core.tokens.{TOK_KW_BIND}
use core.ast.{STMT_BIND}

describe "bind statement parsing":
    it "STMT_BIND has value 20":
        expect(STMT_BIND).to_equal(20)

    it "TOK_KW_BIND has value 204":
        expect(TOK_KW_BIND).to_equal(204)

    it "bind statement is distinct from val":
        val stmt_val = 2
        expect(STMT_BIND).to_equal(20)
        expect(STMT_BIND != stmt_val).to_equal(true)
