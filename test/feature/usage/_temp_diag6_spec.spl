extern fn rt_env_get(key: text) -> text

describe "env check":
    it "SIMPLE_SYSTEM_TEST env":
        val sys = rt_env_get("SIMPLE_SYSTEM_TEST")
        expect(sys).to_equal("")
    it "SIMPLE_SYSTEM_TEST is nil":
        val sys = rt_env_get("SIMPLE_SYSTEM_TEST")
        val is_nil = sys == nil
        val is_empty = sys == ""
        expect(true).to_equal(true)
