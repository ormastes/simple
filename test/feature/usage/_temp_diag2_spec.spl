class Box:
    items: list
    me add(item):
        self.items = self.items + [item]
    fn count() -> i64:
        self.items.len()

describe "val vs var":
    it "val mutation":
        val b = Box(items: [])
        b.add("hello")
        expect(b.count()).to_equal(1)
    it "var mutation":
        var b = Box(items: [])
        b.add("hello")
        expect(b.count()).to_equal(1)
