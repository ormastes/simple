# Test move keyword implementation
# This tests the new move operator for explicit ownership transfer

fn test_move_basic():
    val unique: &Box = new(&) Box(42)
    val moved = move unique  # Should not trigger W1002 warning
    print("Move basic test passed")

fn test_move_in_function():
    val data: &Data = new(&) Data(100)
    take_ownership(move data)  # Explicit move to function
    print("Move in function test passed")

fn take_ownership(d: &Data):
    print("Took ownership of data: {d.value}")

# Data struct for testing
struct Box:
    val value: i32

struct Data:
    val value: i32

# Run tests
test_move_basic()
test_move_in_function()
