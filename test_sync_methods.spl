class MutexGuard<T>:
    value: T

class RwLockReadGuard<T>:
    value: T

class RwLockWriteGuard<T>:
    value: T

class Mutex<T>:
    _handle: Any
    _value: T

    fn try_lock() -> Option<MutexGuard<T>>:
        """Attempt to acquire the lock without blocking."""
        return nil

class RwLock<T>:
    _handle: Any
    _value: T

    fn try_read() -> Option<RwLockReadGuard<T>>:
        """Attempt to acquire a read lock without blocking."""
        return nil

    fn try_write() -> Option<RwLockWriteGuard<T>>:
        """Attempt to acquire a write lock without blocking."""
        return nil

print("Sync methods with nested generics parse successfully!")
